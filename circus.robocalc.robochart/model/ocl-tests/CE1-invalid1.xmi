<?xml version="1.0" encoding="ASCII"?>
<robochart:RCPackage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:robochart="http://www.robocalc.circus/RoboChart">
  <machines
      name="CE1stm1">
    <nodes xsi:type="robochart:Initial"
        name="i0"/>
    <nodes xsi:type="robochart:Final"
        name="f0"/>
    <nodes xsi:type="robochart:State"
        name="s0">
      <actions
          xsi:type="robochart:EntryAction">
        <action
            xsi:type="robochart:IfStmt">
          <expression
              xsi:type="robochart:GreaterThan">
            <left
                xsi:type="robochart:ClockExp"
                clock="//@machines.0/@clocks.0"/>
            <right
                xsi:type="robochart:IntegerExp"
                value="2"/>
          </expression>
          <then
              xsi:type="robochart:Skip"/>
        </action>
      </actions>
    </nodes>
    <transitions
        name="t0"
        source="//@machines.0/@nodes.0"
        target="//@machines.0/@nodes.2"/>
    <transitions
        name="t1"
        source="//@machines.0/@nodes.2"
        target="//@machines.0/@nodes.1"/>
    <clocks
        name="C"/>
  </machines>
</robochart:RCPackage>
