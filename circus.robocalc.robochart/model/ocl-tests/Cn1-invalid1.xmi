<?xml version="1.0" encoding="ASCII"?>
<robochart:RCPackage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:robochart="http://www.robocalc.circus/RoboChart">
  <modules name="Cn1mod1">
    <connections
        from="//@modules.0/@nodes.0"
        to="//@modules.0/@nodes.1/@machines.0"
        efrom="//@modules.0/@nodes.0/@events.0"
        eto="//@modules.0/@nodes.1/@machines.0/@events.0"
        async="true"/>
    <nodes xsi:type="robochart:RoboticPlatformDef"
        name="Cn1rp1">
      <events
          name="event1"/>
    </nodes>
    <nodes xsi:type="robochart:ControllerDef"
        name="Cn1ctrl1">
      <events
          name="event1"/>
      <machines
          xsi:type="robochart:StateMachineDef"
          name="Cn1stm1">
        <events
            name="event1"/>
        <nodes
            xsi:type="robochart:Initial"
            name="i0"/>
        <nodes
            xsi:type="robochart:Final"
            name="f0"/>
        <transitions
            name="t0"
            source="//@modules.0/@nodes.1/@machines.0/@nodes.0"
            target="//@modules.0/@nodes.1/@machines.0/@nodes.1"/>
      </machines>
    </nodes>
  </modules>
</robochart:RCPackage>
